XN,EAWM,IAXN,EAWM,CAVF,UAUE,E,gBATF,mBAAA,CAQS,QART,EAQS;AARD,IAAA,EAAE,EAAC,YAQF;;aARyB,KAAA,CAAA,KAAA,CAAM,U,GAAU,M;;AAQzC,GART,E,WAAA,E;;AAAA,G,mBAAkC,KAAA,CAAA,KAAA,CAAM,U,GAStC,CAXN,C,EAYA,mBAAA,CAGM,KAHN,EAGM,IAHN,EAGM,CAFY,MAAA,CAAA,YAAA,CAAa,E,kBAA3B,mBAAA,CAAkF,QAAlF,EAAkF;UAAA;AAAlD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,QAAA,CAAQ,QAAR,CAAV;AAAA,KAAA,EAAkB,CAAA,SAAA,CAAlB,CAAA;AAA6C,GAAlF,EAAmE,QAAnE,C,mBACA,mBAAA,CAA4D,QAA5D,EAA4D;UAAA;AAA5C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,QAAA,CAAQ,KAAR,CAAV;AAAA,KAAA,EAAkB,CAAA,SAAA,CAAlB,CAAA;AAAuC,GAA5D,EAAgD,KAAhD,C,CACE,CAHN,C,CA/BJ,E;;AAAA,GAoCE,CArCN,C","sourcesContent":["<template>\n    <div class=\"form_wrapper\">\n        <form @keyup.enter.prevent=\"validate()\">\n            <div v-if=\"errors.length > 0\">\n                <ul>\n                    <li v-for=\"error in errors\" :key=\"error\">{{error}}</li>\n                </ul>\n            </div>\n            <span class=\"close\" @click=\"$emit('close-form')\">&#10060;</span>\n            <div>\n                <label for=\"name\">Name:</label>\n                <input type=\"text\" id=\"name\" v-model=\"event.name\">\n            </div>\n            <div>\n                <label for=\"details\">Details:</label>\n                <input type=\"text\" id=\"details\" v-model=\"event.details\">\n            </div>\n            <div>\n                <label for=\"date\">Date:</label>\n                <input type=\"date\" id=\"date\" v-model=\"event.date\">\n            </div>\n            <div>\n                <label for=\"background\">Background Colour:</label>\n                <select id=\"background\"  v-model=\"event.background\">\n                    <option value=\"#F34949\">Red</option>\n                    <option value=\"#F07AEC\">Pink</option>\n                    <option value=\"#997AF0\">Purple</option>\n                    <option value=\"#7AD3F0\">Blue</option>\n                    <option value=\"#68EE94\">Green</option>\n                    <option value=\"#f9f970\">Yellow</option>\n                    <option value=\"#EB9A0F\">Orange</option>\n                </select>\n            </div>\n            <div>\n                <button v-if=\"currentEvent.id\" @click.prevent=\"validate('update')\">Update</button>\n                <button v-else @click.prevent=\"validate('add')\">Add</button>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\n\nexport default {\n    props: [\"currentEvent\"],\n    emits: [\"close-form\", \"add-new-event\", \"update-event\"],\n    mounted() {\n        this.event = this.currentEvent;\n    },\n    data() {\n        return {\n            event: {},\n            errors: []\n        }\n    },\n    methods: {\n        addEvent() {\n            this.$emit('add-new-event', this.event);\n            this.$emit('close-form');\n        },\n        updateEvent() {\n            this.$emit('update-event', this.event);\n            this.$emit('close-form');\n        },\n        validate(type) {\n            this.errors = []\n            if(!this.event.name) this.errors.push('Name is required');\n            if(!this.event.details) this.errors.push('Event details is required');\n            if(!this.event.date) this.errors.push('Event date is required');\n            if(!this.event.background) this.errors.push('Background is required');\n            //has errors, dont add event\n            if(this.errors.length > 0) return;\n            //no errors, add event to array\n            if(type === \"add\")this.addEvent();\n            else this.updateEvent();\n        }\n    }\n}\n</script>\n\n\n<style scoped>\n    .close {\n        align-self: flex-end;\n        cursor: pointer;\n    }\n    .form_wrapper {\n        background: rgba(0,0,0, 0.7);\n        position: absolute;\n        inset: 0;\n        height: 100%;\n        width: 100%;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n    }\n\n    form {\n        display: flex;\n        flex-direction: column;\n        background: white;\n        min-width: 60vw;\n        min-height: 40vh;\n        padding: 2rem;\n        border-radius: 1rem;\n    }\n\n    form > div {\n        display: flex;\n        flex-direction: column;\n        margin: 1rem 0;\n        font-size: 1.6rem;\n    }\n\n    form input,\n    select {\n        margin: .6rem 0;\n        padding: .6rem 1rem;\n        border: 1px lightgray solid;\n        border-radius: .3rem;\n    }\n\n    form button {\n        background: rgb(123, 194, 123);\n        width: 100px;\n        border: none;\n        padding: .6rem;\n        border-radius: .3rem;\n        font-size: 1rem;\n    }\n\n    label {\n        color: rgb(49,49,49);\n    }\n</style>"],"sourceRoot":""}]}