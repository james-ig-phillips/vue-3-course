},\n  {\n    id: 4,\n    name: \"Birthday\",\n    details: \"My birthday party\",\n    date: \"2021-12-19\",\n    background: \"#F07AEC\",\n  },\n  {\n    id: 5,\n    name: \"Christmas\",\n    details: \"ho ho ho\",\n    date: \"2021-12-25\",\n    background: \"#EB9A0F\",\n  },\n  {\n    id: 6,\n    name: \"Conference Talk\",\n    details: \"dont flop\",\n    date: \"2022-02-28\",\n    background: \"#68EE94\",\n  },\n];\n\nimport Event from './components/Event.vue'\n\nexport default {\n    name: 'App',\n    components: {\n        Event\n    },\n    data() {\n        return {\n            events: eventData,\n            showPastEvents: true,\n        }\n    },\n    methods: {\n        daysLeft(event) {\n            const Time = Date.parse(event.date) - Date.now();\n            const Days = Math.ceil(Time / (1000 * 3600 * 24));\n            return Days;\n        }\n    },\n    computed: {\n        orderEvents() {\n\t\t\tconst eventsToOrder = this.events\n\t\t\treturn eventsToOrder.sort((a, b) => a.date > b.date ? 1 : -1);\n\t\t}\n    }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  max-width: 600px;\n  margin: 0 auto;\n}\n\nul {\n    padding: 0;\n}\n\nli {\n    list-style: none;\n    cursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}